-- Delay in seconds
local delayDuration = 5 -- change this to however many seconds you want to delay

-- Delay execution of the rest of the script
print("⏳ Delaying script for " .. delayDuration .. " seconds...")
task.wait(delayDuration)
print("✅ Delay complete. Continuing script...")

local RunService = game:GetService("RunService")

-- Define your moves
local Moves = {
    "Big Bang Kamehameha",
    "Deadly Dance",
    "Big Bang Kamehameha",
    "Blaster Meteor",
    "",
    "",
    "",
    "",
    "",
    ""
}

if game.PlaceId == 536102540 then
    local TeleportService = game:GetService("TeleportService")
    TeleportService:Teleport(3565304751, game.Players.LocalPlayer)
end

if game.PlaceId == 535527772 then
loadstring(game:HttpGet("https://raw.githubusercontent.com/Imsleepyasf/wild/refs/heads/main/npcs"))()
-- References
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local LocalPlayer = Players.LocalPlayer
local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
local Backpack = LocalPlayer.Backpack
local Live = workspace.Live
local Timer = 0

-- Use Moves
local function useMoves()
    for _, moveName in pairs(Moves) do
        if moveName ~= "" then
            local move = Backpack:FindFirstChild(moveName)
            if move then
                move.Parent = Character
                task.wait(0.1)
                move:Activate()
                task.wait(0.2)
                move.Parent = Backpack
                task.wait(0.1)
            end
        end
    end
end

-- Use moves loop
task.spawn(function()
    while task.wait(0.5) do
        useMoves()
    end
end)
end

if game.PlaceId == 3565304751 then
loadstring(game:HttpGet("https://raw.githubusercontent.com/Imsleepyasf/wild/refs/heads/main/queue", true))()
end


-- No-clip
local noclip = true
RunService.Stepped:Connect(function()
    if noclip and Character then
        for _, part in pairs(Character:GetDescendants()) do
            if part:IsA("BasePart") then
                part.CanCollide = false
            end
        end
    end
end)

-- Eat Senzu Loop
task.spawn(function()
    while true do
        if game.PlaceId == 535527772 then
            game.Players.LocalPlayer.Backpack.ServerTraits.EatSenzu:FireServer(true)
        end
        task.wait(0.5)
    end
end)

loadstring(game:HttpGet("https://raw.githubusercontent.com/Imsleepyasf/wild/refs/heads/main/wish", true))()

local Timer = 0  -- Initialize the Timer variable

-- Stopwatch GUI
local function createStopwatch()
    -- Create the GUI elements
    local stopwatchGui = Instance.new("ScreenGui")
    local frame = Instance.new("Frame")
    local textLabel = Instance.new("TextLabel")

    -- Set up the GUI container
    stopwatchGui.Name = "Stopwatch"
    stopwatchGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
    stopwatchGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

    -- Set up the frame
    frame.Parent = stopwatchGui
    frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    frame.BackgroundTransparency = 0.5
    frame.Size = UDim2.new(0, 100, 0, 30)
    frame.Position = UDim2.new(1, -110, 1, -60)  -- Positioned at bottom-right

    -- Set up the text label
    textLabel.Parent = frame
    textLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    textLabel.BackgroundTransparency = 1.000
    textLabel.Size = UDim2.new(1, 0, 1, 0)
    textLabel.Font = Enum.Font.ArialBold
    textLabel.Text = "Timer: 0"
    textLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    textLabel.TextSize = 18
    textLabel.TextWrapped = true

    -- Timer loop that increments every second
    while true do
        wait(1)  -- Wait for 1 second
        Timer = Timer + 1  -- Increment by 1 second
        textLabel.Text = "Timer: " .. Timer  -- Update the label

        -- If Timer reaches 150 seconds, teleport to new place (ID: 536102540)
        if Timer >= 150 then
            game:GetService("TeleportService"):Teleport(536102540, game.Players.LocalPlayer)
            break  -- Stop the loop to prevent further timer updates
        end
    end
end

-- Start stopwatch
createStopwatch()
